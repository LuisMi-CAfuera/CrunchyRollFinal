<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="EditText">
        <item name="android:fontFamily">@font/lato</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textColorHint">@color/white</item>
        <item name="android:backgroundTint">@color/gris</item>
        <item name="android:textCursorDrawable">@drawable/edit</item>
    </style>

    <style name="TextView">
        <item name="android:fontFamily">@font/lato</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="Premium">
        <item name="android:fontFamily">@font/lato_bold</item>
        <item name="android:textColor">@color/amarillo</item>
        <item name="android:textSize">20sp</item>
    </style>

    <style name="Navigation">
         <item name="android:background">@color/grisnav</item>
         <item name="itemIconTint">@color/cambio</item>
         <item name="itemTextColor">@color/cambio</item>
    </style>

    <style name="Constraint_Principal">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:backgroundTint">@color/azulRaro</item>
    </style>

</resources>